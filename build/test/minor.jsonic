    { in: 'a-B_0.', out: 'a-B_0.' }
    { in: ' ?:', out: '%20%3F%3A' }
  ]
}


joinurl: {
  set: [
    { out: 'a' in: ['a'] }
    { out: 'a/b' in: ['a','b'] }
    { out: 'a/b' in: ['a',null,'b'] }
    { out: 'a/b' in: ['a/','b'] }
    { out: 'a/b' in: ['a','/b'] }
    { out: 'a/b' in: ['a/','/b'] }
