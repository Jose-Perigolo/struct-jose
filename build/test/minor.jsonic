    { in: 'a0_', out: 'a0_' }
    { in: '.*+?^${}()|[]\\', out: '\\.\\*\\+\\?\\^\\$\\{\\}\\(\\)\\|\\[\\]\\\\' }
  ]
}


escurl: {
