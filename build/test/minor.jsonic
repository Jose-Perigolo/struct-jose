
isnode: {
  set: [
    { in: {a:1}, out: true }
    { in: [1], out: true }
    { in: 1, out: false }
    { in: 'a', out: false }
    { in: true, out: false }
    { in: null, out: false }
    { out: false }
  ]
}


ismap: {
  set: [
    { in: {a:1}, out: true }
    { in: [1], out: false }
    { in: 1, out: false }
    { in: 'a', out: false }
    { in: true, out: false }
    { in: null, out: false }
    { out: false }
  ]
}


islist: {
  set: [
    { in: {a:1}, out: false }
